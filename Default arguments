#include <iostream>
 using namespace std;
  void sum ( int a, int b, int c= 6, int d = 10);
  int main ( )
  {
  	int a, b, c, d ;
  	cout << " enter any two numbers \n";
  	cin >> a >> b;
  	sum (a, b) ; // sum of default values
  }
  void sum (int a1, int a2, int a3, int a4)
  {
  	int temp;
  	temp = a1 + a2 + a3 + a4;
  	cout << " a = " << a1 << endl;
  	cout << " b = " << a2 << endl;
  	cout << " c = " << a3 << endl;
  	cout << " d = " << a4 << endl;
  	cout << " sum = " << temp;
  	return (0);
  }
